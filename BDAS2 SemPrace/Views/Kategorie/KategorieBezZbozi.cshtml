@model IEnumerable<BDAS2_SemPrace.Models.KategorieBezZboziViewModel>

@{
    ViewData["Title"] = "Index";
}
<div class="container-mt3" style="margin:2rem;">
    <h1>Seznam kategorie bez zboží</h1>

    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Nazev)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Popis)
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Nazev)

                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Popis)
                    </td>
                </tr>
            }
        </tbody>
    </table>
    <div />
